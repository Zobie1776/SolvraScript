// SolvraScript standard library: vector utilities

fn make() {
    return [];
}

fn append(vec, value) {
    return push(vec, value);
}

fn length(vec) {
    return len(vec);
}

fn pop_last(vec) {
    return pop(vec);
}

fn first(vec) {
    if (len(vec) == 0) {
        return null;
    }
    return vec[0];
}
