// SolvraScript core Vec helpers

export fn make() {
    return [];
}

export fn append(vec, value) {
    return push(vec, value);
}

export fn length(vec) {
    return len(vec);
}

export fn pop_last(vec) {
    return pop(vec);
}

export fn first(vec) {
    if (len(vec) == 0) {
        return null;
    }
    return vec[0];
}
