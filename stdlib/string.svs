// SolvraScript standard library: string helpers

fn concat(left, right) {
    return left + right;
}

fn length(text) {
    return len(text);
}

fn repeat(text, times) {
    let mut result = "";
    let mut index = 0;
    while (index < times) {
        result = result + text;
        index = index + 1;
    }
    return result;
}
