// Function to compute factorial
fn factorial(n) {
    let result = 1;
    let i = 1;
    while (i <= n) {
        result = result * i;
        i = i + 1;
    }
    return result;
}

print("Factorials from 1 to 5:");
let x = 1;
while (x <= 5) {
    print("factorial(" + x + ") = " + factorial(x));
    x = x + 1;
}

